{
  "name": "gauge-taiko-template",
  "description": "Starter template for writing JavaScript tests for Gauge",
  "scripts": {
    "dev":"TAIKO_ENABLE_ACTION_OUTPUT=true DEBUG=true headless_chrome=false gauge run specs/UI --env cure/qa -v --tags 'devtest'",
    "local": "gauge run specs --env local -v -p -n=2 --tags 'cure'",
    "qa": "gauge run specs/UI --env cure/qa -v -p -n=3  --tags 'cure'",
    "uat": "gauge run specs/UI --env cure/uat -v -p -n=3 --tags 'cure'",
    "failed": "gauge run --failed",
    "ci": "gauge run specs/UI --env cure/ci -v -p -n=3  --tags 'cure'"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "buffer": "latest",
    "csvtojson": "latest",
    "html-reporter": "^9.10.1",
    "json2csv": "latest",
    "pdf-parse": "^1.1.1",
    "supertest": "^6.3.3",
    "taiko": "^1.3.10",
    "uuid": "latest"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0"
  }
}
